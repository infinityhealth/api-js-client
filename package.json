{
  "name": "@infinityhealth/api-js-client",
  "version": "1.4.30",
  "repository": {
    "url": "git+https://github.com/infinityhealth/api-js-client.git"
  },
  "scripts": {
    "dev": "bun ./scripts/dev",
    "generate": "bun ./scripts/generate",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "bun generate && svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@infinityhealth"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./zod": {
      "types": "./dist/openapi.zod.d.ts",
      "svelte": "./dist/openapi.zod.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "neverthrow": "^8",
    "openapi-client-axios": "^7",
    "zod": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@sveltejs/adapter-auto": "4.0.0",
    "@sveltejs/kit": "2.16.1",
    "@sveltejs/package": "2.3.9",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/bun": "1.2.0",
    "@types/js-yaml": "4.0.9",
    "axios": "1.7.9",
    "bun-plugin-yml": "^0.3.1",
    "highlight.js": "11.11.1",
    "js-yaml": "4.1.0",
    "lefthook": "1.10.10",
    "openapi-zod-client": "1.18.2",
    "openapicmd": "2.6.1",
    "publint": "0.3.2",
    "svelte": "5.19.2",
    "svelte-check": "4.1.4",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "zod": "3.23.8"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
