{
  "name": "@infinityhealth/api-js-client",
  "version": "1.4.11",
  "repository": {
    "url": "git+https://github.com/infinityhealth/api-js-client.git"
  },
  "scripts": {
    "dev": "bun ./scripts/dev",
    "generate": "bun ./scripts/generate",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "bun generate && svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@infinityhealth"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./zod": {
      "types": "./dist/openapi.zod.d.ts",
      "svelte": "./dist/openapi.zod.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "neverthrow": "^8",
    "openapi-client-axios": "^7",
    "zod": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@sveltejs/adapter-auto": "3.3.1",
    "@sveltejs/kit": "2.9.0",
    "@sveltejs/package": "2.3.7",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "@types/bun": "1.1.14",
    "@types/js-yaml": "4.0.9",
    "axios": "1.7.9",
    "bun-plugin-yml": "^0.3.1",
    "highlight.js": "11.10.0",
    "js-yaml": "4.1.0",
    "lefthook": "1.9.0",
    "openapi-zod-client": "1.18.2",
    "openapicmd": "2.6.1",
    "publint": "0.2.12",
    "svelte": "5.9.0",
    "svelte-check": "4.1.1",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "zod": "3.23.8"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
