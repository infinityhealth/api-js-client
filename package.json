{
  "name": "@infinityhealth/api-js-client",
  "version": "0.0.30",
  "repository": {
    "url": "https://github.com/infinityhealth/api-js-client.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "bun ./generate && svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@infinityhealth"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "openapi-client-axios": "^7.5.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/bun": "^1.0.7",
    "@types/js-yaml": "^4.0.9",
    "axios": "^1.6.7",
    "highlight.js": "^11.9.0",
    "js-yaml": "^4.1.0",
    "lefthook": "^1.6.2",
    "openapicmd": "^2.3.1",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
