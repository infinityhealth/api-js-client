{
	"name": "@infinityhealth/api-js-client",
	"version": "1.2.42",
	"repository": {
		"url": "https://github.com/infinityhealth/api-js-client.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "bun ./src/generate && svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/@infinityhealth"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"openapi-client-axios": "7.5.4",
		"neverthrow": "6.1.0",
		"zod": "3.23.6"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@sveltejs/adapter-auto": "3.2.1",
		"@sveltejs/kit": "2.5.10",
		"@sveltejs/package": "2.3.1",
		"@sveltejs/vite-plugin-svelte": "3.1.0",
		"@types/bun": "1.1.3",
		"@types/js-yaml": "4.0.9",
		"axios": "1.7.2",
		"highlight.js": "11.9.0",
		"js-yaml": "4.1.0",
		"lefthook": "1.6.12",
		"openapi-zod-client": "1.18.1",
		"openapicmd": "2.3.1",
		"publint": "0.2.8",
		"svelte": "4.2.17",
		"svelte-check": "3.7.1",
		"tslib": "2.6.2",
		"typed-openapi": "0.4.1",
		"typescript": "5.4.5",
		"vite": "5.2.11",
		"zod": "3.23.6"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
