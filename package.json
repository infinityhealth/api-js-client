{
  "name": "@infinityhealth/api-js-client",
  "version": "1.5.14",
  "repository": {
    "url": "git+https://github.com/infinityhealth/api-js-client.git"
  },
  "scripts": {
    "dev": "bun ./scripts/dev",
    "generate": "bun ./scripts/generate",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "bun generate && svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@infinityhealth"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./zod": {
      "types": "./dist/openapi.zod.d.ts",
      "svelte": "./dist/openapi.zod.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "openapi-client-axios": "^7",
    "zod": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "4.0.0",
    "@sveltejs/kit": "2.17.1",
    "@sveltejs/package": "2.3.10",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/bun": "1.2.2",
    "@types/js-yaml": "4.0.9",
    "axios": "1.7.9",
    "bun-plugin-yml": "^0.3.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "highlight.js": "11.11.1",
    "js-yaml": "4.1.0",
    "openapi-zod-client": "1.18.3",
    "openapicmd": "2.6.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "0.3.4",
    "svelte": "5.19.9",
    "svelte-check": "4.1.4",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "6.1.0",
    "zod": "3.24.1"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
